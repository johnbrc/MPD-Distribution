\pard\tx220\tx720\pardeftab720\li720\fi-720\sl360
\ls72\ilvl0
\i\fs28 \cf2 {\listtext	\'95	}Expansion
\i0  --- organic, steady, cyclical; dependent on specialised roles (queen, drones, workers);\
\ls72\ilvl0
\i {\listtext	\'95	}Harvest
\i0  --- abundant honey, from plant pollen, which is used to feed the growing colony;\
\ls72\ilvl0
\i {\listtext	\'95	}Risks
\i0  --- swarming 
\i divides
\i0  the colony, temporarily weakening both groups.\
\pard\pardeftab720\sl580\sa380

